{
    "$help": "https://aka.ms/terminal-documentation",
    "$schema": "https://aka.ms/terminal-profiles-schema",
    "actions": 
    [
        {
            "command": 
            {
                "action": "copy",
                "singleLine": false
            },
            "keys": "ctrl+c"
        },
        {
            "command": "paste",
            "keys": "ctrl+v"
        },
        {
            "command": "find",
            "keys": "ctrl+shift+f"
        },
        {
            "command": 
            {
                "action": "sendInput",
                "input": "oh-my-posh init pwsh --config \"$env:POSH_THEMES_PATH\\oh-my-posh.omp.json\" | Invoke-Expression && clear\r"
            },
            "keys": "ctrl+shift+/"
        },
        {
            "command": 
            {
                "action": "sendInput",
                "input": "oh-my-posh init pwsh --config \"$env:POSH_THEMES_PATH\\posh-git.omp.json\" | Invoke-Expression && clear\r"
            },
            "keys": "ctrl+/"
        },
        {
            "command": 
            {
                "action": "splitPane",
                "split": "auto",
                "splitMode": "duplicate"
            },
            "keys": "alt+shift+d"
        },
        {
            "command": 
            {
                "action": "sendInput",
                "input": "clear\r"
            },
            "keys": "ctrl+`"
        },
        {
            "command": "toggleAlwaysOnTop",
            "keys": "alt+t"
        }
    ],
    "copyFormatting": "none",
    "copyOnSelect": false,
    "defaultProfile": "{574e775e-4f2a-5b96-ac1e-a2962a402336}",
    "initialCols": 76,
    "initialRows": 26,
    "launchMode": "default",
