{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "transparent",
          "foreground": "#5dfa8a",
          "properties": {
            "style": "folder"
          },
          "style": "plain",
          "template": "\uf7c6",
          "type": "path"
        }
      ],
      "type": "prompt"
    }
  ],
  "transient_prompt": {
    "background": "transparent",
    "foreground": "#5dfa8a",
    "template": "\u279c "
  },
  "final_space": true,
  "version": 2
}
